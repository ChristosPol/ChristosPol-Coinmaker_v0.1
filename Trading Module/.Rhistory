} else if (fut$action[nrow(fut) - 1] %in% c("keep", "buy") &
fut$crossover[nrow(fut)] == "faster_EMA_higher" &
fut$RSI_Flag[nrow(fut)] == "RSI_Above") {
fut$action[nrow(fut)] <- "sell"

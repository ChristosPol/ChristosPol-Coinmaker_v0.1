} else if ( fut$action[nrow(fut) - 1] %in% c("sell", "no action") | (is.na(fut$action[nrow(fut) - 1]) & fut$crossover[nrow(fut)] == "faster_EMA_lower" & fut$crossover_Volume[nrow(fut)] == "volume_lower")) {
fut$action[nrow(fut)] <- "no action"
}
train_data <- fut
}
return(train_data)
